{
  "manifest_version": 3,
  "name": "Absconditus",
  "version": "1.0",
  "description": "A browser extension for the Absconditus password manager.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "icons": {
    "128": "icon.png"
  }
}